<svg width="800" height="600" viewBox="0 0 800 600" fill="none" xmlns="http://www.w3.org/2000/svg">
<defs>
<linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
<stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
<stop offset="100%" style="stop-color:#FF8B36;stop-opacity:0.8" />
</linearGradient>
</defs>
<rect width="800" height="600" fill="url(#sky)"/>
<!-- Historic Building -->
<rect x="100" y="200" width="600" height="300" fill="#F5F5EB" stroke="#D5D5D5" stroke-width="3"/>
<!-- Roof -->
<polygon points="50,200 400,100 750,200" fill="#D54330" stroke="#A03020" stroke-width="2"/>
<!-- Columns -->
<rect x="200" y="300" width="40" height="200" fill="#E8E8E8"/>
<rect x="300" y="300" width="40" height="200" fill="#E8E8E8"/>
<rect x="400" y="300" width="40" height="200" fill="#E8E8E8"/>
<rect x="500" y="300" width="40" height="200" fill="#E8E8E8"/>
<!-- Entrance -->
<rect x="350" y="400" width="100" height="100" fill="#8B6914"/>
<!-- Sign -->
<rect x="150" y="350" width="200" height="80" fill="#2F554A"/>
<text x="250" y="380" text-anchor="middle" fill="#F5F5EB" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Museo de</text>
<text x="250" y="410" text-anchor="middle" fill="#F5F5EB" font-family="Arial, sans-serif" font-size="16" font-weight="bold">San Juan</text>
<!-- Ground -->
<rect x="0" y="500" width="800" height="100" fill="#D2B48C"/>
</svg>
